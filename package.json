{
  "name": "dynalite",
  "version": "1.0.3",
  "description": "An implementation of Amazon's DynamoDB built on LevelDB",
  "main": "index.js",
  "bin": "cli.js",
  "scripts": {
    "build": "for file in ./db/*.pegjs; do pegjs \"$file\"; done",
    "test": "mocha --require should --reporter spec -t $([ $REMOTE ] && echo 30s || echo 4s)",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- --require should -t 4s",
    "lint": "eslint ."
  },
  "repository": "mhart/dynalite",
  "keywords": [
    "dynamodb",
    "mock",
    "fake",
    "test",
    "aws",
    "dynamo",
    "leveldb"
  ],
  "author": "Michael Hart <michael.hart.au@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "async": "^1.5.2",
    "big.js": "^3.1.3",
    "buffer-crc32": "^0.2.5",
    "lazy": "^1.0.11",
    "level-sublevel": "^6.5.4",
    "levelup": "^1.3.1",
    "lock": "^0.1.2",
    "memdown": "^1.1.2",
    "minimist": "^1.2.0",
    "once": "^1.3.3"
  },
  "optionalDependencies": {
    "leveldown": "^1.4.4"
  },
  "devDependencies": {
    "aws4": "^1.3.2",
    "eslint": "^3.12.1",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "pegjs": "^0.9.0",
    "should": "^8.3.1"
  }
}
